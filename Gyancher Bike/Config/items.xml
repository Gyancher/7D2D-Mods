<configs>
	
<append xpath="/items">
	
		<item name="manux_vehicleQuadPlaceable">
			<property name="Extends" value="vehicleMinibikePlaceable"/>
			<property name="CustomIcon" value="manux_Quad"/>
			<property name="DescriptionKey" value="manux_vehicleQuadDesc"/>
			<property name="Meshfile" value="#@modfolder:Resources/manux_Quad.unity3d?QuadPrefab_A17.prefab"/>
			<property name="EconomicValue" value="15000"/>
			<property class="Action1">
				<property name="Vehicle" value="manux_vehicleQuad"/>
				<property name="VehicleSize" value="1.1, 1.3, 2"/>
			</property>
			<effect_group name="Base Effects">
				<passive_effect name="DegradationMax" operation="base_set" value="3000,6000" tier="1,6"/>
				<passive_effect name="ModSlots" operation="base_set" value="4" tier="1,2,3,4,5,6"/>
				<passive_effect name="ModPowerBonus" operation="perc_add" value=".10" tags="EntityDamage,BlockDamage"/>
				<passive_effect name="ModPowerBonus" operation="base_add" value="300" tags="EconomicValue"/>
			</effect_group>			
		</item>
		
		
		<item name="manux_vehicleMonsterBeetlePlaceable">
			<property name="Extends" value="vehicleMinibikePlaceable"/>
			<property name="CustomIcon" value="manux_Quad"/>
			<property name="DescriptionKey" value="manux_vehicleQuadDesc"/>
				<property name="Meshfile" value="#@modfolder:Resources/manux_MonsterBeetle.unity3d?MonsterBeetle_A17.prefab"/>
			<property name="EconomicValue" value="15000"/>
			<property class="Action1">
				<property name="Vehicle" value="manux_vehicleMonsterBeetle"/>
				<property name="VehicleSize" value="1, 1, 1"/>
			</property>
			<effect_group name="Base Effects">
				<passive_effect name="DegradationMax" operation="base_set" value="3000,6000" tier="1,6"/>
				<passive_effect name="ModSlots" operation="base_set" value="4" tier="1,2,3,4,5,6"/>
				<passive_effect name="ModPowerBonus" operation="perc_add" value=".10" tags="EntityDamage,BlockDamage"/>
				<passive_effect name="ModPowerBonus" operation="base_add" value="300" tags="EconomicValue"/>
			</effect_group>			
		</item>
		
		
</append>

</configs>